// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Feedback {
  id        String   @id @default(uuid())
  name      String
  email     String
  message   String
  rating    Int // de 1 a 5
  createdAt DateTime @default(now())
}
